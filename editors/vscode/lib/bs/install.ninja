rescript = 1
rule cp
  command = cp $i $out
rule touch
 command = touch $out
o VSCode.cmi : cp ../bs/src/VSCode.cmi
o VSCode.cmj : cp ../bs/src/VSCode.cmj
o VSCode.cmt : cp ../bs/src/VSCode.cmt
